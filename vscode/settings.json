{
    "workbench.colorTheme": "Visual Studio Light",
    "workbench.startupEditor": "none",
    "notebook.lineNumbers": "on",
    "explorer.confirmDragAndDrop": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "remote.SSH.remotePlatform": {
        "10.20.14.42": "linux",
        "10.26.6.81": "linux",
        "10.20.14.43": "linux",
        "120.79.52.236": "linux",
        "120.78.204.43": "linux",
        "10.20.14.63": "linux"
    },
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "editor.wordWrap": "on",
    "git.openRepositoryInParentFolders": "never",
    "[python]": {
        "editor.formatOnType": true
    },
    "git.ignoreLegacyWarning": true,
    "window.zoomLevel": 1,
"latex-workshop.latex.tools": [
  {
    "name": "pdflatex",
    "command": "pdflatex",
    "args": [
      "-interaction=nonstopmode",
      "-file-line-error",
      "-synctex=1",
      "%DOC%"
    ]
  },
  {
    "name": "xelatex",
    "command": "xelatex",
    "args": [
      "-synctex=1",
      "-interaction=nonstopmode",
      "-file-line-error",
      "%DOC%"
    ]
  },
  {
    "name": "latexmk",
    "command": "latexmk",
    "args": [
      "-synctex=1",
      "-interaction=nonstopmode",
      "-file-line-error",
      "-pdf",
      "%DOC%"
    ]
  },
  {
    "name": "biber",
    "command": "biber",
    "args": [
      "%DOCFILE%"
    ]
  }
],
"latex-workshop.latex.recipes": [
    {"tools": ["latexmk"]},
  {
    "name": "pdflatex ➜ biber ➜ pdflatex×2",
    "tools": ["xelatex", "biber", "xelatex", "xelatex"]
  }
],
    "latex-workshop.view.pdf.viewer": "browser",
    "latex-workshop.view.pdf.autoSync": true,
    "latex-workshop.view.pdf.autoRefresh": true,
    "latex-workshop.latex.clean.enabled": true,
// "latex-workshop.latex.clean.onFailBuild": true,
"latex-workshop.latex.clean.fileTypes": [
  "*.aux",
  "*.bbl",
  "*.blg",
  "*.idx",
  "*.ind",
  "*.lof",
  "*.lot",
  "*.out",
  "*.toc",
  "*.acn",
  "*.acr",
  "*.alg",
  "*.glg",
  "*.glo",
  "*.gls",
  "*.fls",
  "*.log",
  "*.fdb_latexmk"
],
    // "latex-workshop.latex.clean.fileTypes": [],
    "latex-workshop.latex.autoClean.run": "onBuilt",
    // "latex-workshop.latex.autoBuild.run": "onSave",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.shell.windows": "C:\\WINDOWS\\System32\\cmd.exe",
    "breadcrumbs.enabled": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "remote.SSH.configFile": "D:\\.ssh\\config",
    "git.untrackedChanges": "separate",
    "spellright.language": [],
    "spellright.documentTypes": [
        "markdown",
        "plaintext"
    ],
    "cmake.configureOnOpen": true,
    "cmake.showOptionsMovedNotification": false,
    "files.watcherInclude": [
        "/home/maojingwei/project/nal/*.py*"
    ],
    "Lingma.LocalStoragePath": "/home/maojingwei/.lingma",
    "files.autoSave": "onFocusChange",
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.lineEnding": "lf",
    "editor.fontSize": 14,
    "git.ignoreLimitWarning": true,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false,
        "<C-l>": false,
        "<C-c>": false,
        "<C-f>": false,
        "<C-p>": false,
        "<C-a>": false,
        "<C-x>": false,
        "<C-v>": false,
        "<C-b>": true
    },
    "vim.useSystemClipboard": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "y"
            ],
            "after": [
                "g",
                "t"
            ]
        },
        {
            "before": [
                "g",
                "t"
            ],
            "after": [
                "g",
                "T"
            ]
        },
        {
            "before": [
                ";",
                "q"
            ],
            "commands": [
                ":q"
            ]
        },
        {
            "before": [
                "g",
                "j"
            ],
            "after": [
                "<CR>",
                "<LeftMouse>"
            ]
        }
    ],
    "pasteImage.path": "${currentFileDir}/mdfigures",
    "files.watcherExclude": {
        "**/.git/objects/**": false,
        "**/.git/subtree-cache/**": false,
        "**/.hg/store/**": false,
        "**/node_modules/*/**": false
    },
    "workbench.editorAssociations": {
      "*.pdf": "default"
    }
}
